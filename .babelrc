{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["require-context-hook"]
    }
  }
}
